[run]
branch = True

omit =
    /kb/module/lib/kb_MiniASM/authclient.py
    /kb/module/lib/kb_MiniASM/baseclient.py
    /kb/module/lib/kb_MiniASM/kb_MiniASMClient.py
    /kb/module/lib/kb_MiniASM/kb_MiniASMServer.py
source = /kb/module/lib/kb_MiniASM


[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError

show_missing = 1
